# VPN Master Panel - Environment Configuration

# API Settings
API_PORT=8000
WEB_PORT=3000
DEBUG=false

# Database
DB_PASSWORD=CHANGE_THIS_PASSWORD

# Security (Generate with: openssl rand -hex 32)
SECRET_KEY=CHANGE_THIS_TO_RANDOM_32_CHARACTERS

# Admin Account
INITIAL_ADMIN_USERNAME=admin
INITIAL_ADMIN_PASSWORD=CHANGE_THIS_PASSWORD
INITIAL_ADMIN_EMAIL=admin@vpnmaster.local

# VPN Service Ports
OPENVPN_PORT=1194
WIREGUARD_PORT=51820
L2TP_PSK=vpnmaster
CISCO_PORT=4443

# Iran Bypass Features
DOMAIN_FRONTING_ENABLED=true
TLS_OBFUSCATION_ENABLED=true
AUTO_SWITCH_ON_BLOCK=true

# Monitoring (Optional)
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001
GRAFANA_PASSWORD=admin

# Logging
LOG_LEVEL=INFO
